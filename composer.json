{
    "name": "codeapi/laravel-redshift",
    "description": "Fork from merlijnbuit/laravel-redshift and YuK1Game/laravel-redshift. This is a package to handle Redshift in Laravel like eloquent.",
    "license": "MIT",
    "authors": [
        {
            "name": "YuK1Game",
            "email": "develop@yuk1.jp"
        }
    ],
    "homepage": "https://github.com/ThomasBUEE/laravel-redshift",
    "keywords": ["Laravel", "Redshift"],
    "require": {
        "illuminate/support": "~7|~8|~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "YuK1\\LaravelRedshift\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "YuK1\\LaravelRedshift\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YuK1\\LaravelRedshift\\LaravelRedshiftServiceProvider"
            ]
        }
    }
}
